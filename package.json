{
  "name": "fxql-parser",
  "version": "0.0.1",
  "description": "Foreign Exchange Query Language Parser",
  "author": "Dayo-Adewuyi",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "npm install && npm install -g @nestjs/cli",
    "build": " npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.json",
    "test:watch": "jest --watch --config./jest.config.json",
    "test:cov": "jest --coverage --config ./jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./jest.config.json --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand",
    "migration:create": "npm run typeorm migration:create src/database/migrations/initial-schema.migrations.ts",
    "migration:generate": "npm run typeorm migration:generate src/database/migrations  -- -d src/database/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/database/data-source.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:migrate": "./scripts/db-migrate.sh",
    "db:seed": "./scripts/seed-data.sh",
    "seed:run": "ts-node -r tsconfig-paths/register  seed -d src/config/database/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "nest-winston": "^1.9.7",
    "pg": "^8.13.1",
    "rate-limiter-flexible": "^5.0.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm-extension": "^3.6.3",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
